      "values": [
        "oui",
        "non"
      ],
      "required": true,
      "value": "non",
      "attribut_label": "Embacle branchage"
    },
    "sous_berges": {
      "type_widget": "radio",
      "values": [
        "oui",
        "non"
      ],
      "required": true,
      "value": "non",
      "attribut_label": "Sous berges"
    },
    "colmatage": {
      "type_widget": "radio",
      "values": [
        "oui",
        "non"
      ],
      "required": true,
      "value": "non",
      "attribut_label": "Colmatage"
    },
    "evolution": {
      "type_widget": "text",
      "attribut_label": "Evolution"
    },
    "largeur_du_cours_d_eau_cm": {
      "type_widget": "number",
      "attribut_label": "Largeur du cours d'eau (cm)"
