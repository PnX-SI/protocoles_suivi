  ],
  "display_list": [
    "id_base_site",
    "visit_date_min",
    "observers"
  ],
  "map_label_field_name": "base_site_code",
  "chain_show": [
    "visit_date_min",
    "id_base_site"
  ],
  "keep": [
    "visit_date_min",
    "observers",
    "id_dataset",
    "comment"
  ],
  "specific": {
    "periode": {
      "type_widget": "select",
      "values": [
        "Nocturne",
        "Diurne"
      ],
      "attribut_label": "Période"
    },
    "lineaire_prospection": {
      "type_widget": "number",
      "attribut_label": "Lineaire prospection"
    },
    "participants_nom": {
      "type_widget": "text",
      "attribut_label": "Participant s(nom)"
    },
    "organisme": {
      "type_widget": "text",
      "attribut_label": "Organisme"
    },
    "id_dataset": {
      "hidden": "({meta}) => meta.dataset && Object.keys(meta.dataset).length == 1"
    },
    "conditions_d_observation": {
      "type_widget": "select",
      "values": [
        "Bonnes",
        "Moyennes",
        "Mauvaises"
      ],
      "attribut_label": "Conditions d'observation"
    },
    "technique": {
      "type_widget": "select",
      "values": [
        "Pêche sondage",
        "Visuelle",
        "Nasses"
      ],
      "attribut_label": "Technique"
    },
    "chevelu_racinaire": {
      "type_widget": "radio",
      "values": [
        "oui",
        "non"
      ],
      "required": true,
      "value": "non",
      "attribut_label": "Chevelu racinaire"
    },
    "vegetation_aquatique": {
      "type_widget": "radio",
      "values": [
        "oui",
        "non"
      ],
      "required": true,
      "value": "non",
      "attribut_label": "Vegetation aquatique"
    },
    "embacle_branchage": {
